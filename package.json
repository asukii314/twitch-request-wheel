{
  "name": "twitch-bot",
  "version": "0.3.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "node-fetch": "^2.6.1",
    "query-string": "^4.3.4",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.17",
    "react-wheel-of-prizes": "git+https://github.com/dcpesses/react-wheel-of-prizes.git",
    "tmi.js": "^1.8.2",
    "web-vitals": "^1.1.1",
    "yaml": "^1.10.2"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "start": "env-cmd --fallback -f .env.local react-scripts start",
    "prebuild": "npm run test:ci",
    "build": "react-scripts build",
    "test": "env-cmd --fallback -f .env.local react-scripts test",
    "test:ci": "./tests/test.sh",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "jest": {
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.js",
      "\\.(css|less)$": "<rootDir>/tests/style-mock.js",
      "\\.(yaml|yml)$": "<rootDir>/tests/yaml-mock.js"
    }
  }
}
